    paths:
        - ../code/src
    excludePaths:
        - '../code/src/Shared/Tests/*'
        - '../code/src/Profile/UserStatus/Tests/*'
        - '../code/src/Site/Registration/Tests/*'
    treatPhpDocTypesAsCertain: false